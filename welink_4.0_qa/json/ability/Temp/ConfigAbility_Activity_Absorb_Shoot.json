                                        {
                                            "$type": "ByTargetGlobalValue",
                                            "key": "_ABILITY_Common_Channel_IsTrigger",
                                            "value": 0,
                                            "compareType": "Equal",
                                        },
                                    ],
                                },
                                {
                                    "$type": "SetGlobalValue",
                                    "key": "_ABILITY_Common_Channel_IsTrigger",
                                    "value": 1,
                                    "doOffStage": true,
                                },
                            ],
                            "chargeStateIDs": ["DiveChannel_Loop"], 
