    {//70800604	3.8灯光玩法白盒_蓝插片
        "Default":{
            "$type": "ConfigAbility",
            "abilityName": "SceneObj_Area_Xm_Property_Ani_Prop_LightPlay_Card_01",
            "abilitySpecials": {
                "SGV_COLOR":0,
                "_CARD_COLOR":0
            },
            "onAdded": [
                {
                    "$type": "SetGlobalValueToOverrideMap",
                    "globalValueKey": "SGV_COLOR",
                    "overrideMapKey": "SGV_COLOR",
                    "canBeHandledOnRecover": true
                }
            ],
            "abilityMixins": [
                {
                    "$type": "AttachToGadgetStateMixin",
                    "gadgetState": 201,
                    "modifierName": "State_201",
                },
                {
                    "$type": "AttachToGadgetStateMixin",
                    "gadgetState": 0,
                    "modifierName": "State_0",
                },
            ],
            "onFieldEnter": [
                {
                    "$type": "Predicated",
                    "target": "Target",
                    "targetPredicates": [
                        {
                            "$type": "ByEntityTypes",
                            "entityTypes": ["Avatar"],
                        }
                    ],
                    "successActions": [
                        {
                            "$type": "CopyGlobalValue",
                            "srcKey": "SGV_COLOR",
                            "srcTarget": "Self",
                            "dstKey": "_CARD_COLOR",
                            "dstTarget": "Other",
                            "target": "Other"
                        },
                        {
                            "$type": "ApplyModifier",
                            "modifierName": "_CopyGV",
                            "target": "Other"
                        },
                        {//重新上modifier使applier为自己
                            "$type": "RemoveModifier",
                            "modifierName": "UNIQUE_Team_State_Card",
                            "target": "TargetTeam"
                        },
                        {
                            "$type": "ApplyModifier",
                            "modifierName": "UNIQUE_Team_State_Card",
                            "target": "TargetTeam",
                        },
                        {
                            "$type": "ChangeGadgetState",
                            "state": 201,
                        }
                    ],
                    "predicates": [
                        {
                            "$type": "ByTargetGadgetState",
                            "gadgetState": 0,
                            "target": "Self"
                        }
                    ]
                }
            ],
            "modifiers": {
                "_CopyGV":{
                    "stacking": "Multiple",
                    "duration": 0.1,
                    "onAdded": [
                        {
                            "$type": "CopyGlobalValue",
                            "srcKey": "_CARD_COLOR",
                            "srcTarget": "Self",
                            "dstKey": "_CARD_COLOR",
                            "dstTarget": "Team"
                        },
                    ]
                },
                "State_0":{
                    "onAdded": [
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_SceneObj_GlideChampOrb_01",
                            "predicates": [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "SGV_COLOR",
                                    "compareType": "Equal",
                                    "value": 3,
                                    "target": "Self",
                                }
                            ],
                            "canBeHandledOnRecover": true
                        },
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_SceneObj_GlideChampOrb_02",
                            "predicates": [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "SGV_COLOR",
                                    "compareType": "Equal",
                                    "value": 2,
                                    "target": "Self",
                                }
                            ],
                            "canBeHandledOnRecover": true
                        },
                    ]
                },
                "State_201":{},
                "UNIQUE_Team_State_Card":{//发给team的modifier
                    "isUnique": true,
                    "thinkInterval": 0.5,
                    "onThinkInterval": [//不能携带太远
                        {
                            "$type": "SetGlobalValueByTargetDistance",
                            "key": "_Card_Distance",
                            "target": "Applier",
                        },
                        {
                            "$type": "Predicated",
                            "target": "Self",
                            "targetPredicates": [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_Card_Distance",
                                    "compareType": "MoreThan",
                                    "value": 30,
                                }
                            ],
                            "successActions": [
                                {//恢复插片state
                                    "$type": "ChangeGadgetState",
                                    "state": 0,
                                    "target": "Applier",
                                },
                                {//重置插片gv
                                    "$type": "SetGlobalValue",
                                    "key": "_CARD_COLOR",
                                    "value": 0
                                },
                            ]
                        }
                    ],
                    "onAdded": [
                        {
                            "$type": "AttachModifier",
                            "modifierName": "UNIQUE_Team_State_Card_AttachEff"
                        }
                    ],
                    "modifierMixins": [
                        {//被错误的光照射后移除自己，恢复插片state
                            "$type": "DoActionOnGlobalValueChangeMixin",
                            "globalValueKey": "_LIGHT_COLOR",
                            "actions": [
                                {
                                    "$type": "SetGlobalValueToOverrideMap",
                                    "globalValueKey": "_CARD_COLOR",
                                    "overrideMapKey": "_CARD_COLOR"
                                },
                                {
                                    "$type": "Predicated",
                                    "targetPredicates": [
                                        {
                                            "$type": "ByAny",
                                            "predicates": [
                                                {//等于，代表光照正确
                                                    "$type": "ByTargetGlobalValue",
                                                    "key": "_LIGHT_COLOR",
                                                    "compareType": "Equal",
                                                    "value": "%_CARD_COLOR"
                                                },
                                                {//为零，代表没被光照
                                                    "$type": "ByTargetGlobalValue",
                                                    "key": "_LIGHT_COLOR",
                                                    "compareType": "Equal",
                                                    "value": 0
                                                },
                                            ]
                                        }
                                    ],
                                    "failActions": [//都不是，代表被错误的光照射
                                        {//恢复插片state
                                            "$type": "ChangeGadgetState",
                                            "state": 0,
                                            "target": "Applier",
                                        },
                                        {//重置插片gv
                                            "$type": "SetGlobalValue",
                                            "key": "_CARD_COLOR",
                                            "value": 0
                                        },
                                    ]
                                }
                            ]
                        },
                        {//修改插片gv————变成其他值代表吃了别的插片，恢复原插片；变成0代表被使用
                            "$type": "DoActionOnGlobalValueChangeMixin",
                            "globalValueKey": "_CARD_COLOR",
                            "actions": [
                                //改颜色
                                {
                                    "$type": "RemoveModifier",
                                    "modifierName": "UNIQUE_Team_State_Card_AttachEff",
                                },
                                {
                                    "$type": "AttachModifier",
                                    "modifierName": "UNIQUE_Team_State_Card_AttachEff",
                                },
                                //0时移除自己
                                {
                                    "$type": "Predicated",
                                    "targetPredicates": [
                                        {
                                            "$type": "ByTargetGlobalValue",
                                            "key": "_CARD_COLOR",
                                            "compareType": "NoneOrEqual",
                                            "value": 0
                                        }
                                    ],
                                    "successActions": [
                                        {//移除自己（放在最后）
                                            "$type": "RemoveModifier",
                                            "modifierName": "UNIQUE_Team_State_Card",
                                        },
                                    ],
                                    "failActions": [
                                        {//恢复插片state
                                            "$type": "ChangeGadgetState",
                                            "state": 0,
                                            "target": "Applier",
                                        },
                                    ]
                                }
                            ]
                        },
                    ],
                },
                "UNIQUE_Team_State_Card_AttachEff":{
                    "isUnique": true,
                    "modifierMixins": [
                        {
                            "$type": "CurLocalAvatarMixinV2",
                            "modifierName": "UNIQUE_Avatar_State_Card_AttachEff"
                        }
                    ],
                },
                "UNIQUE_Avatar_State_Card_AttachEff":{
                    "isUnique": true,
                    "onAdded": [//各种颜色特效
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_SceneObj_GlideChampOrb_01_WhiteBox",
                            "predicates": [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_CARD_COLOR",
                                    "compareType": "Equal",
                                    "value": 3,
                                    "target": "Team",
                                }
                            ],
                            "canBeHandledOnRecover": true
                        },
                        {
                            "$type": "AttachEffect",
                            "effectPattern": "Eff_SceneObj_GlideChampOrb_02_WhiteBox",
                            "predicates": [
                                {
                                    "$type": "ByTargetGlobalValue",
                                    "key": "_CARD_COLOR",
                                    "compareType": "Equal",
                                    "value": 2,
                                    "target": "Team",
                                }
                            ],
                            "canBeHandledOnRecover": true
                        },
                    ],
                }
            }
        }
